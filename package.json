{
  "name": "65xx-kit",
  "version": "1.0.0",
  "description": "A repo with tools for aiding 65xx cpu development",
  "main": "index.js",
  "scripts": {
    "setup": "run-script-os",
    "setup:darwin": "brew install acme vice minipro && true",
    "setup:win32": "choco install acme-assembler -y -s \".\\tools\" && choco install winvice -y",
    "build": "run-script-os",
    "build:darwin": "mkdir -p build && acme --cpu 6502 --format cbm",
    "build:win32": "(mkdir build) & acme --cpu 6502 --format cbm",
    "start": "x64sc",
    "rom:gen":"node src/rom.js",
    "rom:write": "minipro -p AT28C256 -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bberak/65xx-kit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bberak/65xx-kit/issues"
  },
  "homepage": "https://github.com/bberak/65xx-kit#readme",
  "devDependencies": {
    "run-script-os": "^1.1.1"
  }
}
